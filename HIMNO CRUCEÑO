#include <iostream> // Libreria para escribir y leer 

using namespace std; //Que la entrada va por el teclado y la salida por el monitor

int main()
{
    cout<< "Bajo el cielo mas puro de América" << endl;
    cout<< "y en la tierra de Ñuflo de Chavez," << endl;
    cout<< "Libertad van trinando las aves" << endl;
    cout<<"de su veste ostentando el primor." << endl;
    
    return 0;
}
